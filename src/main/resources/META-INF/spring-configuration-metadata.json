{
  "properties": [
    {
      "name": "image.storage.path",
      "type": "java.lang.String",
      "description": "Path to the local image storage directory.",
      "defaultValue": "/default/path"
    },
    {
      "name": "image.download.url",
      "type": "java.lang.String",
      "description": "URL to access images.",
      "defaultValue": "http://localhost:8080/images/downloads/"
    },
    {
      "name": "password.reset.request.confirm.url",
      "type": "java.lang.String",
      "description": "URL to confirm password reset request.",
      "defaultValue": "http://localhost:3000/reset-password/confirm/"
    },
    {
      "name": "password.update.verify.url",
      "type": "java.lang.String",
      "description": "URL to verify password update.",
      "defaultValue": "http://localhost:8080/api/auth/verify-password-update?token="
    },
    {
      "name": "account.verify.url",
      "type": "java.lang.String",
      "description": "URL to verify user account.",
      "defaultValue": "http://localhost:3000/verify-account/"
    }
  ]
}
